<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org/DTD Mapper 3.0// EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace = "com.dreamsecurity.mapper.kmsagent">
	
	<select id="selectKeyOwnerUsingUserUid" parameterType="keyOwnerVo" resultType="keyOwnerVo">
		select
			id,
			key_uid keyUid,
			user_uid userUid,
			create_date createDate,
			state
		from 
			ext_kms_key_owner
		where
			user_uid = #{userUid} and
			key_uid = #{keyUid}
	</select>
		
</mapper>